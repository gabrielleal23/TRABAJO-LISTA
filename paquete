package principal;

/**
 *
 * @author SALA
 */
public class carro {
    
    public static String motor;
    public String tipo;
    int id;
    
    //Constructores 
    public carro (String entradamotor, String entradatipo){
        this.motor=entradamotor;
        this.tipo=entradatipo;
        
    }
    
    void velocidad(int velocidadinicialcarrera,int velocidadinicialcurva){
         for(int recta=velocidadinicialcarrera; recta<=velocidadinicialcurva;recta+=30){
             System.out.println("Velocidad del carro"+recta);
         }
    }
    
}

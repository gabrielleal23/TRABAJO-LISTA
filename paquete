        System.out.println("El tipo de vehiculo es "+ objetocarro.tipo);
        objetocarro.id=(3421);
               
        objetocarro.velocidad(0, 130);
        
        competidor objetocompetidor=new competidor("Pepito","o+","a5");
        competidor.compa√±ero="Jaime";
        System.out.println("El nombre del competidor es " + objetocompetidor.nombrecorredor);
        System.out.println("El rh del competidor es "+  objetocompetidor.rh);
        
        
        
        
        
        
